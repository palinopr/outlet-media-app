# Your Ticketmaster promoter login
TM_EMAIL=your@email.com
TM_PASSWORD=yourpassword

# Where to send the scraped data (your live app or local dev)
INGEST_URL=http://localhost:3000/api/ingest
# INGEST_URL=https://your-app.vercel.app/api/ingest

# Secret token - must match INGEST_SECRET in the Next.js app .env
INGEST_SECRET=change-this-to-a-random-string

# Optional: run headless (true = no visible browser, false = you see the browser)
HEADLESS=false
